{
    "name": "as283/artisan-plantuml",
    "description": "Laravel Artisan commands for generating migrations and Eloquent models from PlantUML class diagrams and vice-versa.",
    "license": "GPL",
    "autoload": {
        "psr-4": {
            "As283\\ArtisanPlantuml\\": "src/"
        },
        "files": [
            "src/Util/LaravelTypes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "As283\\ArtisanPlantuml\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrejs Sikorskis",
            "email": "as283@alu.ua.es"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/as283-ua/PlantUML-Processor.git"
        }
    ],
    "require": {
        "as283/plant-uml-processor": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "As283\\ArtisanPlantuml\\PUMLServiceProvider"
            ]
        }
    }
}